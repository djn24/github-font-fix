{
    "name": "GitHub Monospace Font Fix",
    "version": "0.1",
    "description": "Replace the GitHub monospace font with one that draws extended ASCII diagrams correctly.",
    "icons": {
        "32": "GitHub-Mark-32px.png",
        "64": "GitHub-Mark-64px.png",
        "120": "GitHub-Mark-120px-plus.png"
    },
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*",
                "https://gist.github.com/*"
            ],
            "css": [
                "fixfont.css"
            ]
        }
    ]
}